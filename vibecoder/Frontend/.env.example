# VibeCoder Marketplace - Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000/api
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Razorpay Configuration
NEXT_PUBLIC_RAZORPAY_KEY=rzp_test_your_key_id

# Application Configuration
NEXT_PUBLIC_SITE_NAME=VibeCoder Marketplace
NEXT_PUBLIC_SITE_DESCRIPTION=Premium coding projects marketplace
NEXT_PUBLIC_DOMAIN=vibecodeseller.com

# Analytics (Optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_CHAT=false
NEXT_PUBLIC_MAINTENANCE_MODE=false
