{
  "name": "vibecoder-marketplace",
  "version": "1.0.0",
  "description": "Digital marketplace for Indian coders and developers",
  "main": "backend/src/server.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run install-deps && npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm install && npx prisma generate",
    "build:frontend": "cd frontend && npm install && npm run build",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "install-deps": "cd backend && npm install && cd ../frontend && npm install",
    "deploy": "npm run build && npm run start",
    "postinstall": "npm run install-deps",
    "heroku-postbuild": "npm run build",
    "prestart": "cd backend && npx prisma generate",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install",
    "setup:frontend": "cd frontend && npm install",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd backend && npm run format",
    "format:frontend": "cd frontend && npm run format",
    "prepare": "husky"
  },
  "keywords": [
    "marketplace",
    "ecommerce",
    "coding",
    "india",
    "digital-products"
  ],
  "author": "VibeCoder Team",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4"
  },
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manan0901/Vibecoder.git"
  },
  "homepage": "https://vibecodeseller.com",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.9.0",
    "framer-motion": "^12.16.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3"
  }
}
